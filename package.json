{
  "name": "coreframework",
  "version": "10.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=18192 node_modules/@angular/cli/bin/ng serve",
    "build": "node --max_old_space_size=18192 node_modules/@angular/cli/bin/ng build",
    "test": "ng test --source-map=false",
    "lint": "ng lint",
    "serve": "ng serve",
    "e2e": "ng e2e",
    "clean": "rimraf ./dist/",
    "nmclean": "rimraf ./node_modules/",
    "indexFileGeneration": "ts-node ./src/framework/assets/scripts/indexFileGen.js",
    "adalRedirectFile": "ts-node ./src/framework/assets/scripts/adalRedirectGen.js",
    "buildInfoAndClean": "npm run clean && npm run buildInfo",
    "build:dev": "run-s lint \"indexFileGeneration -- --env=dev\" \"adalRedirectFile -- --env=dev\" buildInfoAndClean \"build -- -c=dev\"",
    "build:dev1": "run-s lint \"indexFileGeneration -- --env=dev1\" \"adalRedirectFile -- --env=dev1\" buildInfoAndClean \"build -- -c=dev1\"",
    "build:dev2": "run-s lint \"indexFileGeneration -- --env=dev2\" \"adalRedirectFile -- --env=dev2\" buildInfoAndClean \"build -- -c=dev2\"",
    "build:qa": "run-s lint \"indexFileGeneration -- --env=qa\" \"adalRedirectFile -- --env=qa\" buildInfoAndClean \"build -- -c=qa\"",
    "build:qa1": "run-s lint \"indexFileGeneration -- --env=qa1\" \"adalRedirectFile -- --env=qa1\" buildInfoAndClean \"build -- -c=qa1\"",
    "build:qa2": "run-s lint \"indexFileGeneration -- --env=qa2\" \"adalRedirectFile -- --env=qa2\" buildInfoAndClean \"build -- -c=qa2\"",
    "build:prd": "run-s lint \"indexFileGeneration -- --env=prod\" \"adalRedirectFile -- --env=prod\" buildInfoAndClean \"build -- -c=production\"",
    "build:prd1": "run-s lint \"indexFileGeneration -- --env=prod1\" \"adalRedirectFile -- --env=prod1\" buildInfoAndClean \"build -- -c=prod1\"",
    "build:prd2": "run-s lint \"indexFileGeneration -- --env=prod2\" \"adalRedirectFile -- --env=prod2\" buildInfoAndClean \"build -- -c=prod2\"",
    "local": "run-s indexFileGeneration adalRedirectFile buildInfo serve",
    "docs": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -a docs_images --disableCoverage --hideGenerator",
    "coverage": "ng test --code-coverage",
    "testandcoverage": "ng test --source-map=false && ng test --code-coverage",
    "analyze": "npm run build -- --prod --stats-json && webpack-bundle-analyzer ./dist/stats-es2015.json",
    "buildInfo": "ts-node build.js",
    "serviceworker": "npm run build:prd && cd dist && http-server -p 4200"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.4",
    "@angular/cdk": "^10.2.1",
    "@angular/common": "^10.2.4",
    "@angular/compiler": "^10.2.4",
    "@angular/core": "^10.2.4",
    "@angular/forms": "^10.2.4",
    "@angular/platform-browser": "^10.2.4",
    "@angular/platform-browser-dynamic": "^10.2.4",
    "@angular/router": "^10.2.4",
    "@angular/service-worker": "^10.2.4",
    "@azure/msal-browser": "^2.7.0",
    "@fortawesome/fontawesome-pro": "^5.13.0",
    "@microsoft/applicationinsights-web": "^2.5.4",
    "@ng-select/ng-select": "^6.0.0",
    "ag-grid-angular": "^25.0.1",
    "ag-grid-community": "^25.0.1",
    "angular-powerbi": "^1.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.3.4",
    "jquery": "^3.5.1",
    "lodash-es": "^4.17.20",
    "msal": "^1.4.5",
    "msalx": "^0.1.2",
    "ng-dynamic-component": "^7.0.2-no-barrels.2",
    "ng2-dragula": "^2.1.1",
    "ngx-bootstrap": "^5.3.2",
    "ngx-powerbi": "^3.0.0",
    "ngx-slick-carousel": "^0.5.1",
    "ngx-toastr": "^11.1.2",
    "rxjs": "^6.5.5",
    "slick-carousel": "^1.8.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.1",
    "@angular-devkit/core": "10.2.1",
    "@angular/cli": "^10.2.1",
    "@angular/compiler-cli": "^10.2.4",
    "@angular/language-service": "^10.2.4",
    "@compodoc/compodoc": "1.1.11",
    "@types/jasmine": "~3.4.4",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-powerbi": "3.0.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.0",
    "source-map-explorer": "^2.4.2",
    "ts-node": "~8.4.1",
    "tslint": "~6.1.0",
    "typescript": "^4.0.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "yargs": "^14.2.0"
  }
}